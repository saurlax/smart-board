<view class="stt-body">
  <!-- 应到 实到 人流量-折线图    手机 姓名 时间 体温 -->
  <view>
    <canvas canvas-id="columnCanvas" disable-scroll="true" class="canvas"></canvas>
  </view>
  <view class="tab-bar">
    <view class="tab tab-selected" bindtap="toStatistics">
      <image class="tab-svg"
        src="data:image/svg+xml;base64," />
      <text>统计</text>
    </view>
    <view class="tab" bindtap="toActivity">
      <image class="tab-svg"
        src="data:image/svg+xml;base64," />
      <text>活动</text>
    </view>
    <view class="tab" bindtap="toMy">
      <image class="tab-svg"
        src="data:image/svg+xml;base64," />
      <text>我的</text>
    </view>
  </view>
</view>